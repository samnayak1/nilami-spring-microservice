{
  "realm": "nilami-microservices-security-realm",
  "enabled": true,
  "roles": {
    "realm": [
      {"name": "CUSTOMER"},
      {"name": "SELLER"},
      {"name": "ADMIN"}
    ]
  },
  "clients": [
    {
      "clientId": "nilami-client",
      "enabled": true,
      "secret": "${KEYCLOAK_CLIENT_SECRET}",
      "publicClient": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,    
      "authorizationServicesEnabled": true  
    }
  ]
}
