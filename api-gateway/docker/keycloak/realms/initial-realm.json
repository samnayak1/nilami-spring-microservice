{
  "realm": "nilami-microservices-security-realm",
  "enabled": true,
  "roles": {
    "realm": [
      {"name": "CUSTOMER"},
      {"name": "SELLER"},
      {"name": "ADMIN"}
    ]
  },
  "clients": [
    {
      "clientId": "nilami-client",
      "enabled": true,
      "secret": "${KEYCLOAK_CLIENT_SECRET}",
      "publicClient": false,
      "directAccessGrantsEnabled": true
    }
  ],
  "users": [
    {
      "username": "admin",
      "email": "admin@nilami.com",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "${KEYCLOAK_ADMIN_PASSWORD}",
          "temporary": false
        }
      ],
      "realmRoles": ["ADMIN"]
    }
  ]
}
